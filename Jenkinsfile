pipeline {
  agent any
  stages {
    stage("build") {
      steps {
        echo 'building application'
        echo 'building complete'
      }
    }
    
    stage("test") {
      steps {
        echo 'testing application'
      }
    }
    
    stage("deploy") {
      steps {
        echo 'deploying application'
      }
    }
  }
}
